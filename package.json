{
  "name": "tickflow-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "create-api": "json-server --host 0.0.0.0 -p 3001 --watch src/mockdata/db.json",
    "lint": "eslint '**/*.{tsx,ts,js}'",
    "format": "prettier '**/*.{tsx,ts,js,json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install",
    "style": "npx tailwindcss -i src/index.css -o src/index.css"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.16",
    "axios": "^1.3.4",
    "flowbite": "^1.6.4",
    "flowbite-react": "^0.4.2",
    "json-server": "^0.17.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.5",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
